<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:SplitLayoutPanel ui:field="layout">
        <g:center>
            <g:DockLayoutPanel>
                <g:center>
                    <g:FlowPanel ui:field="nodeUrnTablePanel"></g:FlowPanel>
                </g:center>
                <g:south size="25">
                    <g:Button ui:field="reloadButton">Reload</g:Button>
                </g:south>
            </g:DockLayoutPanel>
        </g:center>
        <g:west size="250">
            <g:SplitLayoutPanel ui:field="innerlayout">
                <g:center>
                    <g:FlowPanel ui:field="configurationListPanel"></g:FlowPanel>
                </g:center>
                <g:south size="300">
                    <g:DockLayoutPanel>
                        <g:center>
                            <g:Label ui:field="description"></g:Label>
                        </g:center>
                        <g:south size="25">
                            <g:Button ui:field="loginButton">Login...</g:Button>
                        </g:south>
                    </g:DockLayoutPanel>
                </g:south>
            </g:SplitLayoutPanel>
        </g:west>
    </g:SplitLayoutPanel>
</ui:UiBinder>